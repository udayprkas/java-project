<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_info</servlet-name>
        <servlet-class>login_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>contact_info.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>contact_info</servlet-name>
        <servlet-class>contact_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ride_info</servlet-name>
        <servlet-class>ride_info</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup_info</servlet-name>
        <servlet-class>signup_info</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_info</servlet-name>
        <url-pattern>/login_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>contact_info</servlet-name>
        <url-pattern>/contact_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ride_info</servlet-name>
        <url-pattern>/ride_info</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup_info</servlet-name>
        <url-pattern>/signup_info</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
